<%= form_with model: @user, url: sign_up_path do |f|  %>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  <p>  
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </p>
    <%= f.submit "Sign Up" %>
<% end %>